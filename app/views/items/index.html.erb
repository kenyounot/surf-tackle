<h1>All the Surf Tackle created!</h1>

<ul>
    <% @items.each do |item| %>
        <li>
            <%= item.name  + " - " + item.brand.name %><br>
            <%= item.description %>
            <%= link_to "Write a review about this", new_item_review_path(item.id) %>
            <%= link_to "View reviews for this item", item_reviews_path(item.id) %>
        </li>
    <% end %>
</ul>